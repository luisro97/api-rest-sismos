{
    "name": "api-rest",
    "version": "1.0.0",
    "description": "API RESTful que realiza web-scraping a una pagina de sismos para luego insertar los datos en una base de datos postgresql y disponer de ellos",
    "main": "src/index.js",
    "scripts": {
        "dev": "nodemon src/index.js --exec babel-node",
        "build": "babel src --out-dir dist",
        "modules": "npm i cheerio request request-promise express pg pg-hstore sequelize morgan @babel/polyfill jsonwebtoken",
        "dev_modules": "npm i --save-dev @babel/core @babel/cli @babel/preset-env nodemon @babel/node",
        "start": "nodemon src/index.js --exec babel-node"
    },
    "keywords": [],
    "author": "Luis Salinas, Ariel Painenao, Cristobal Sanchez",
    "license": "ISC",
    "dependencies": {
        "@babel/polyfill": "^7.12.1",
        "bcryptjs": "^2.4.3",
        "cheerio": "^1.0.0-rc.10",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "pg": "^8.6.0",
        "pg-hstore": "^2.3.4",
        "request": "^2.88.2",
        "request-promise": "^4.2.6",
        "router": "^1.3.5",
        "sequelize": "^6.6.5",
        "swagger-jsdoc": "^6.1.0",
        "swagger-ui-express": "^4.1.6"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.5",
